{
  "name": "server",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf ./coverage ./dist ./node_modules",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:repl": "nest start --watch --entryFile=repl",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node ./dist/main",
    "seed": "ts-node -r tsconfig-paths/register ./src/seeds/seeds.script",
    "format": "prettier --write \"./src/**/*.ts\"",
    "check:types": "tsc --noemit",
    "check:eslint": "eslint \"./src/**/*.ts\" --fix",
    "lint": "npm run check:types && npm run check:eslint",
    "test:watch": "jest --watch --config=./jest.config.json",
    "test:cov": "jest --coverage --config=./jest.config.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config=./jest.config.json"
  },
  "dependencies": {
    "@bull-board/api": "^5.21.0",
    "@bull-board/express": "^5.21.0",
    "@bull-board/nestjs": "^5.21.0",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.15.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "mongoose": "^8.5.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.4.1",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  }
}
