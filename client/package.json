{
  "name": "client",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "clean": "rimraf \"./{dist,node_modules}\"",
    "build": "tsc && vite build",
    "start:dev": "vite",
    "start:preview": "vite preview",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
